<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="plan_view_agile_place_request_history_view_form" model="ir.ui.view">
        <field name="name">plan_view_agile_place_request_history_form</field>
        <field name="model">plan.view.agile.place.request_history</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="type" />
                        <field name="session_id" />
                        <field name="request_server_date" />
                        <field name="status_code" />
                        <field name="is_success" />
                        <field name="send_data" />
                        <field name="response_data" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="plan_view_agile_place_request_history_view_graph" model="ir.ui.view">
        <field name="name">plan_view_agile_place_request_history_graph</field>
        <field name="model">plan.view.agile.place.request_history</field>
        <field name="arch" type="xml">
            <graph string="Plan view agile place request history">
                <field name="name" type="row" />
                <field name="session_id" type="row" />
            </graph>
        </field>
    </record>

    <record id="plan_view_agile_place_request_history_view_kanban" model="ir.ui.view">
        <field name="name">plan_view_agile_place_request_history_kanban</field>
        <field name="model">plan.view.agile.place.request_history</field>
        <field name="arch" type="xml">
            <kanban class="o_kanban_mobile">
                <field name="name" />
                <field name="session_id" />
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div class="oe_kanban_details">
                                <ul>
                                    <li class="mb4">
                                        <strong>
                                            <field name="name" />
                                        </strong>
                                    </li>
                                    <li class="mb4">
                                        <strong>
                                            <field name="session_id" />
                                        </strong>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <record id="plan_view_agile_place_request_history_view_pivot" model="ir.ui.view">
        <field name="name">plan_view_agile_place_request_history_pivot</field>
        <field name="model">plan.view.agile.place.request_history</field>
        <field name="arch" type="xml">
            <pivot string="Plan view agile place request history">
                <field name="name" type="row" />
                <field name="session_id" type="row" />
            </pivot>
        </field>
    </record>

    <record id="plan_view_agile_place_request_history_view_search" model="ir.ui.view">
        <field name="name">plan_view_agile_place_request_history_search</field>
        <field name="model">plan.view.agile.place.request_history</field>
        <field name="arch" type="xml">
            <search string="Plan view agile place request history">
                <filter domain="[('name','!=',False)]" name="name" string="Name" />
                <filter domain="[('session_id','!=',False)]" name="session_id" string="Session" />
            </search>
        </field>
    </record>

    <record id="plan_view_agile_place_request_history_view_tree" model="ir.ui.view">
        <field name="name">plan_view_agile_place_request_history_tree</field>
        <field name="model">plan.view.agile.place.request_history</field>
        <field name="arch" type="xml">
            <tree>
                <field name="request_server_date" />
                <field name="name" />
                <field name="type" />
                <field name="status_code" />
                <field name="is_success" />
                <field name="session_id" />
            </tree>
        </field>
    </record>

    <act_window
        id="plan_view_agile_place_request_history_view_agile_place_request_history_action_window"
        name="View agile place request history"
        res_model="plan.view.agile.place.request_history"
        view_mode="kanban,tree,pivot,graph,form"
    />
</odoo>
